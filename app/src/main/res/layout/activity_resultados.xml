<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    android:orientation="vertical"
    tools:context=".ResultadosActivity"
    tools:ignore="RtlSymmetry">

    <TextView
        android:id="@+id/tv_Resultados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center_horizontal"
        android:text="@string/resultados"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="395dp" />

    <TextView
        android:id="@+id/tv_marckis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="34dp"
        android:text="@string/marckis"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tv_Resultados"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/colorMarckis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="89dp"
        android:layout_marginBottom="79dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/colorMandelin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/tv_Mecke"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/mecke"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnRetomar"
        app:layout_constraintStart_toStartOf="@+id/btnRetomar"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/colorMecke"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/colorMarckis"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/colorMarckis"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/tv_mandelin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:text="@string/mandelin"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/tv_marckis"
        app:layout_constraintTop_toBottomOf="@+id/tv_marckis"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/colorMandelin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="168dp"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/tv_simon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginTop="55dp"
        android:text="@string/simon"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/tv_Mecke"
        app:layout_constraintTop_toBottomOf="@+id/tv_Mecke"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/colorSimon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/colorMandelin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/colorMandelin"
        tools:layout_conversion_absoluteHeight="31dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/tvCoincidencias"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="35dp"
        android:text="@string/coincidencias"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tvResMarckis"
        app:layout_constraintStart_toEndOf="@+id/tvResMarckis"
        tools:layout_conversion_absoluteHeight="37dp"
        tools:layout_conversion_absoluteWidth="395dp" />

    <TextView
        android:id="@+id/tvResMarckis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="240dp"
        android:text="@string/marckis"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/resCoiMar"
        app:layout_constraintStart_toStartOf="@+id/resCoiMar"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="192dp" />

    <ListView
        android:id="@+id/resCoiMar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="37dp"
        app:layout_constraintBottom_toTopOf="@+id/resCoiMan"
        app:layout_constraintEnd_toStartOf="@+id/resCoiMec"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvResMarckis"
        tools:layout_conversion_absoluteHeight="124dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <TextView
        android:id="@+id/tvResMecke"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="@string/mecke"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/resCoiMec"
        app:layout_constraintStart_toStartOf="@+id/resCoiMec"
        app:layout_constraintTop_toBottomOf="@+id/tvCoincidencias"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="192dp" />

    <ListView
        android:id="@+id/resCoiMec"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="36dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvResSimon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/resCoiMar"
        app:layout_constraintTop_toTopOf="@+id/tvResMecke"
        tools:layout_conversion_absoluteHeight="124dp"
        tools:layout_conversion_absoluteWidth="192dp" />

    <TextView
        android:id="@+id/tvResMandelin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="1dp"
        android:text="@string/mandelin"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/resCoiMan"
        app:layout_constraintEnd_toEndOf="@+id/btnEnviar"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <ListView
        android:id="@+id/resCoiMan"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintBottom_toBottomOf="@+id/resCoiSim"
        app:layout_constraintEnd_toStartOf="@+id/btnRetomar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/resCoiSim"
        tools:layout_conversion_absoluteHeight="124dp"
        tools:layout_conversion_absoluteWidth="193dp" />

    <TextView
        android:id="@+id/tvResSimon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="214dp"
        android:layout_marginBottom="215dp"
        android:text="@string/simon"
        android:textAlignment="center"
        android:textColor="@color/primaryLightColor"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btnRetomar"
        app:layout_constraintEnd_toEndOf="@+id/tv_simon"
        app:layout_constraintStart_toStartOf="@+id/tv_simon"
        app:layout_constraintTop_toBottomOf="@+id/tvResMecke"
        tools:layout_conversion_absoluteHeight="20dp"
        tools:layout_conversion_absoluteWidth="194dp" />

    <ListView
        android:id="@+id/resCoiSim"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnEnviar"
        app:layout_constraintTop_toBottomOf="@+id/resCoiMec"
        tools:layout_conversion_absoluteHeight="124dp"
        tools:layout_conversion_absoluteWidth="189dp" />


    <Button
        android:id="@+id/btnEnviar"
        style="?android:attr/buttonBarButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="3dp"
        android:paddingStart="25dp"
        android:text="@string/enviar_resultados"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/resCoiSim"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_conversion_absoluteHeight="54dp"
        tools:layout_conversion_absoluteWidth="207dp" />

    <Button
        android:id="@+id/btnRetomar"
        style="?android:attr/buttonBarButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="3dp"
        android:paddingEnd="25dp"
        android:text="@string/nueva_muestra"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/resCoiMan"
        tools:layout_conversion_absoluteHeight="54dp"
        tools:layout_conversion_absoluteWidth="199dp" />


</android.support.constraint.ConstraintLayout>